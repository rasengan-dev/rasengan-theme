{
  "name": "@rasenganjs/theme",
  "private": false,
  "version": "1.0.0",
  "description": "Library that handles themes for Rasengan.JS applications",
  "type": "module",
  "main": "lib/index.js",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    }
  },
  "author": {
    "name": "dilane3",
    "email": "komboudilane125@gmail.com",
